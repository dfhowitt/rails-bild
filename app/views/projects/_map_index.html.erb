<h2>Results for "<%= @query %>":</h2>
<% unless @results %>
  <div class="">
    <p class="no-results"><i class="fas fa-times text-danger"></i>  No results for "<%= @query %></p>
  </div>
  <h2>We suggest:</h2>
<% end %>
<div id="map"
 style="width: 100%; height: 100vh;"
 data-markers="<%= @markers.to_json %>"
 data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>">
</div>
